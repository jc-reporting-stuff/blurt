{
  "name": "hours",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build": "npx prisma db push && npx prisma generate && npm run build && npm run vercel-postbuild"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-vercel": "^1.0.0-next.36",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "i": "^0.3.7",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "prisma": "^3.7.0",
    "svelte": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^3.7.0"
  }
}
